            <zeroOrMore>
               <ref name="teimodel.descLike"/>
            </zeroOrMore>
      
      
        
      
      
        
      
      
            <zeroOrMore>
               <ref name="teifigure"/>
            </zeroOrMore>
      
      
            <zeroOrMore>
               <ref name="teimodel.graphicLike"/>
