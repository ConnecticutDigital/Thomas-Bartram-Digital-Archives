                  <ref name="teimodel.divTop"/>
                  <ref name="teimodel.global"/>
               </choice>
            </zeroOrMore>
            <choice>
               <oneOrMore>
                  <group>
                     <ref name="teiitem"/>
                     <zeroOrMore>
                        <ref name="teimodel.global"/>
                     </zeroOrMore>
                  </group>
               </oneOrMore>
               <group>
                  <oneOrMore>
                     <group>
                        <zeroOrMore>
                           <ref name="teimodel.global"/>
                        </zeroOrMore>
                        <ref name="teiitem"/>
                        <zeroOrMore>
                           <ref name="teimodel.global"/>
                        </zeroOrMore>
