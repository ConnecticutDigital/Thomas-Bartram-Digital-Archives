            <zeroOrMore>
               <ref name="teimodel.headLike"/>
            </zeroOrMore>
            <choice>
               <zeroOrMore>
                  <ref name="teimodel.pLike"/>
               </zeroOrMore>
               <zeroOrMore>
                  <choice>
                     <ref name="teimodel.labelLike"/>
                     <ref name="teimodel.placeStateLike"/>
                     <ref name="teimodel.eventLike"/>
