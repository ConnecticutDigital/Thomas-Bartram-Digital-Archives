         <ref name="teiatt.global.attributes"/>
         <ref name="teiatt.naming.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="teicollection">
      <element name="collection">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">contains the name of a collection of manuscripts, not necessarily located within a single repository. [10.4. The Manuscript Identifier]</a:documentation>
         <ref name="teimacro.xtext"/>
         <ref name="teiatt.global.attributes"/>
         <ref name="teiatt.naming.attributes"/>
         <ref name="teiatt.typed.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="teiphysDesc">
      <element name="physDesc">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(physical description) contains a full physical description of a manuscript or manuscript part, optionally subdivided using more specialized elements from the model.physDescPart class. [10.7. Physical Description]</a:documentation>
      
        <zeroOrMore>
            <ref name="teimodel.pLike"/>
         </zeroOrMore>
      
      
        
        
      
         <ref name="teiatt.global.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="teiatt.global.facs.attributes">
      <ref name="teiatt.global.facs.attribute.facs"/>
   </define>
   <define name="teiatt.global.facs.attribute.facs">
      <optional>
         <attribute name="facs">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(facsimile) points to all or part of an image which corresponds with the content of the element.</a:documentation>
            <list>
               <oneOrMore>
                  <data type="anyURI"/>
               </oneOrMore>
            </list>
         </attribute>
      </optional>
   </define>
   <define name="teiatt.global.change.attributes">
      <ref name="teiatt.global.change.attribute.change"/>
   </define>
   <define name="teiatt.global.change.attribute.change">
      <optional>
         <attribute name="change">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">points to one or more change elements documenting a state or revision campaign to which the element bearing this attribute and its children have been assigned by the encoder.</a:documentation>
            <list>
               <oneOrMore>
                  <data type="anyURI"/>
               </oneOrMore>
            </list>
         </attribute>
      </optional>
   </define>
   <define name="teihandNotes">
      <element name="handNotes">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">contains one or more handNote elements documenting the different hands identified within the source texts. [11.3.2.1. Document Hands]</a:documentation>
         <oneOrMore>
            <ref name="teihandNote"/>
         </oneOrMore>
         <ref name="teiatt.global.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="teisupplied">
      <element name="supplied">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">signifies text supplied by the transcriber or editor for any reason; for example because the original cannot be read due to physical damage, or because of an obvious omission by the author or scribe. [11.3.3.1. Damage, Illegibility, and Supplied Text]</a:documentation>
         <ref name="teimacro.paraContent"/>
         <ref name="teiatt.global.attribute.xmlid"/>
         <ref name="teiatt.global.attribute.n"/>
         <ref name="teiatt.global.attribute.xmllang"/>
         <ref name="teiatt.global.attribute.xmlbase"/>
         <ref name="teiatt.global.attribute.xmlspace"/>
         <ref name="teiatt.global.rendition.attribute.rend"/>
         <ref name="teiatt.global.rendition.attribute.style"/>
         <ref name="teiatt.global.rendition.attribute.rendition"/>
         <ref name="teiatt.global.facs.attribute.facs"/>
         <ref name="teiatt.global.change.attribute.change"/>
         <ref name="teiatt.global.responsibility.attribute.cert"/>
         <ref name="teiatt.editLike.attributes"/>
         <attribute name="resp">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
            <choice>
               <value>#RF</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <value>#RP</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <value>#SG</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
            </choice>
         </attribute>
         <optional>
            <attribute name="reason">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">one or more words indicating why the text has had to be supplied, e.g. overbinding, faded-ink, lost-folio, omitted-in-original.</a:documentation>
               <list>
                  <oneOrMore>
                     <data type="token">
                        <param name="pattern">(\p{L}|\p{N}|\p{P}|\p{S})+</param>
                     </data>
                  </oneOrMore>
               </list>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <start>
      <choice>
         <ref name="teiTEI"/>
      </choice>
   </start>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="bartramODD-divPage-constraint-8">
      <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                xmlns:rng="http://relaxng.org/ns/structure/1.0"
                context="tei:div[@type='page']">
                     <sch:assert test="current()[@facs]"> The
                        div element indicating a transcribed/encoded page must have a facs attribute pointing to the manuscript image.</sch:assert>
                     <sch:assert test="count(child::tei:div[@type='group']) ge 1"> The
                        div element indicating a transcribed/encoded page must have at least one child div @type="group", which contains the group of entries for any single account.</sch:assert>
                  </sch:rule>
   </pattern>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="bartramODD-divGroup-constraint-9">
      <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                xmlns:rng="http://relaxng.org/ns/structure/1.0"
                context="tei:div[@type='group']">
                     <sch:assert test="count(child::tei:head) eq 1"> Every div @type="group" must have a child head element.</sch:assert>
                     <sch:assert test="count(child::tei:list[@type='entry']) ge 1"> Every div @type="group" must have at least one child list @type="entry".</sch:assert>
                  </sch:rule>
   </pattern>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="bartramODD-lists-constraint-10">
      <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                xmlns:rng="http://relaxng.org/ns/structure/1.0"
                context="tei:list">
                     <sch:report test="current()[not(child::tei:head)][parent::div[@type='group'][not(child::tei:head)]]"> This list element must have a child head element because the parent div @type="group" does not have a head element.</sch:report>
                     <sch:assert test="count(child::tei:item) ge 1"> Every list must have at least one child item element.</sch:assert>
                  </sch:rule>
   </pattern>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="bartramODD-date-constraint-11">
      <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                xmlns:rng="http://relaxng.org/ns/structure/1.0"
                context="tei:date[descendant::tei:div]">
                     <sch:assert test="current()[@when]"> Every date element inside of page transcription must get a when attribute with a iso standard value.</sch:assert>
                  </sch:rule>
   </pattern>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="bartramODD-unclearText-constraint-12">
      <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                xmlns:rng="http://relaxng.org/ns/structure/1.0"
                context="tei:unclear">
                     <sch:assert test="current()[@reason][@unit][@quantity]"> The
                        unclear element must have a reason, unit, and quantity attribute.</sch:assert>
                  </sch:rule>
   </pattern>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="bartramODD-suppliedResp-constraint-13">
      <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                xmlns:rng="http://relaxng.org/ns/structure/1.0"
                context="tei:supplied">
                     <sch:assert test="current()[@resp]"> The
                        supplied element must have a resp attribute.</sch:assert>
                  </sch:rule>
   </pattern>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="bartramODD-addResp-constraint-14">
      <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                xmlns:rng="http://relaxng.org/ns/structure/1.0"
                context="tei:add">
                     <sch:assert test="current()[@resp]"> The
                        supplied element must have a resp attribute.</sch:assert>
                  </sch:rule>
   </pattern>
</grammar>
