       <!-- struck through -->
       <choice rend="align(right)">
        <abbr>D</abbr>
        <expan>debtor</expan>
       </choice>
      </p>
      <p> To
       order<!-- what is bookkeeping markup here? --><!-- GV: What does it mean in terms of economic activities? -->
